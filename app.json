{
  "expo": {
    "name": "recipe-book",
    "slug": "recipe-book",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.richardsefton.recipebook"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.richardsefton.recipebook"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photoPermission": "Allow the app access to your photos to attach recipe images.",
          "cameraPermission": "Allow the app access to your camera to take recipe images."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "c5bbf012-9b68-400f-84d3-52068d710670"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/c5bbf012-9b68-400f-84d3-52068d710670"
    }
  }
}
